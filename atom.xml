<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
<channel>
 <title>Henrik Andersson</title>
 <link href="http://henri.kandersson.com/"/>
 <lastBuildDate>{{ site.time | date_to_xmlschema }}</updated>
 <guid>http://henri.kandersson.com/</id>
 <managingEditor>ahandersson@gmail.com</managingEditor>

 {% for post in site.posts %}
 <item>
   <title>{{ post.title }}</title>
   <link href="http://henri.kandersson.com{{ post.url }}"/>
   <description>{{ post.content | xml_escape }}</content>
   <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
   <guid>http://henri.kandersson.com{{ post.id }}</id> 
 </item>
 {% endfor %}
 
</channel>
</rss>
